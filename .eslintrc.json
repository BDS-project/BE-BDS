{
    "env": {
      "commonjs": true,
      "es2021": true,
      "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
      "ecmaVersion": 12
    },
    "rules": {
      "no-console": "off",
      "consistent-return": "off",
      "no-underscore-dangle": "off",
      "import/extensions": "off",
      "max-len": ["error", { "code": 150 }],
      "comma-dangle": ["error", "never"],
      "object-curly-newline": ["error", { "multiline": true }] 
    }
  }